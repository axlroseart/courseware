@require './mixins.styl'
.AwardCon
  .starXu
    position absolute
    left 0
    top 0
  #xu,.videoCon,video,.starXu
    width 100%
    height 100%
  .oneCon
    position absolute
    top 357px
    left 791px
    .imgUrl
      width 295px
      height 295px
      border-radius 295px
    .name
      font-size 100px
      text-align center
      color white
  .threeCon
    .lollCon
      position absolute
      top: 202px;
      left: 803px;
      width 286px
      height 542px
    .lollCenter
      transform-origin: center bottom;
    .lollCenterAnima {
      animation-name: lollCenterKey;
      animation-duration: 1.1s;
      animation-delay: 0s;
      animation-iteration-count: infinite;
      animation-fill-mode: forwards;
    }
    @keyframes lollCenterKey {
      0% {
        transform: rotate(0deg);
      }
      50% {
        transform: rotate(-3deg);
      }
      100% {
        transform: rotate(0deg);
      }
    }
    .lollLeft
      left: 620px;
      top: 207px;
      transform: rotate(-24deg);
      transform-origin: center bottom;
    .lollLeftAnima {
      animation-name: lollLeftKey;
      animation-duration: 1.1s;
      animation-delay: 0s;
      animation-iteration-count: infinite;
      animation-fill-mode: forwards;
    }
    @keyframes lollLeftKey {
      0% {
        transform: rotate(-24deg);
      }
      50% {
        transform: rotate(-32deg);
      }
      100% {
        transform: rotate(-24deg);
      }
    }
    .lollRight
      transform: rotate(19deg);
      transform-origin: center bottom;
      left: 1011px;
      top: 214px;
    .lollRightAnima {
      animation-name: lollRightKey;
      animation-duration: 1.1s;
      animation-delay: 0s;
      animation-iteration-count: infinite;
      animation-fill-mode: forwards;
    }
    @keyframes lollRightKey {
      0% {
        transform: rotate(19deg);
      }
      50% {
        transform: rotate(25deg);
      }
      100% {
        transform: rotate(19deg);
      }
    }
    .Lollipop
      position absolute
      width 286px
      height 542px
    .starLeft
      position absolute
      width 117px
      height: 121px;
      transform: rotate(-30deg);
      left: -5px;
      top: -63px;
    .starCenter
      position absolute
      width 117px
      height: 121px;
      top: -87px;
      left: 94px;
    .starRight
      position absolute
      width 117px
      height: 121px;
      transform: rotate(30deg);
      left: 188px;
      top: -50px;
    .good
      position absolute
      top: 525px;
      left: 840px;
      width 195px
      height 281px
    .imgUrlThree
      width 232px
      height 232px
      border-radius 232px
      position absolute
      left 25px
      top 25px
    .threeText
      position absolute
      width 100%
      font-size 80px
      text-align center
      color white
      top 255px