{
  "name": "IC-Courseware",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && node ./publish.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@xkeshi/vue-countdown": "^1.0.0",
    "animejs": "^2.2.0",
    "bindings": "^1.3.0",
    "countup.js": "^1.9.3",
    "element-ui": "^2.4.1",
    "gsap": "^2.0.1",
    "lodash": "^4.17.11",
    "odometer": "^0.4.8",
    "protobufjs": "^6.8.6",
    "sat": "^0.7.1",
    "stylus-px2rem": "^1.0.14",
    "team-js-signals": "^1.0.7",
    "velocity-animate": "^1.5.2",
    "videojs-flash": "^2.1.1",
    "vue": "^2.5.16",
    "vue-cookie": "^1.1.4",
    "vue-count-to": "^1.0.13",
    "vue-router": "^3.0.1",
    "vue-video-player": "^5.0.2",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-standard": "^3.0.0-rc.3",
    "babel-plugin-component": "^1.1.1",
    "canvas-game-aide": "^1.0.0",
    "css-loader": "^0.28.11",
    "easeljs": "^1.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "howler": "^2.0.13",
    "net": "^1.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "globals": {
      "ICsocket": false,
      "Howl": false
    },
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "no-console": "off",
      "no-new": 0,
      "no-global-assign": [
        "error"
      ],
      "vue/no-parsing-error": [
        0,
        {
          "x-invalid-end-tag": false,
          "duplicate-attribute": false
        }
      ],
      "vue/valid-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    ""
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
